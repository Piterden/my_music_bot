{
  "name": "my-music-bot",
  "version": "1.0.0",
  "description": "Telegram music bot",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "forever start --minUptime 30000 --spinSleepTime 30000 index.js",
    "build": "forever start --minUptime 30000 --spinSleepTime 30000 -l forever.log -o out.log -w -v -d -e err.log index.js",
    "watch": "forever start --minUptime 30000 --spinSleepTime 30000 -w -v -d index.js"
  },
  "keywords": [
    "telegram",
    "player",
    "files",
    "music"
  ],
  "author": "Denis Efremov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Piterden/my_music_bot.git"
  },
  "dependencies": {
    "dotenv": "^4.0.0",
    "klaw": "^2.1.1",
    "nodemon": "^1.12.1",
    "telegraf": "^3.15.3",
    "telegraf-flow": "^8.3.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "eslint": "^3.19.0",
    "eslint-config-coffeeandcode": "^0.2.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-vue": "^2.1.0"
  }
}
